	left: 6px;
	font-weight: 700;
	min-width: 47px;
	width: auto;
}
.megamenu .row {
	margin: 0;
}
.custom-cart-btn {
	width: 100%;
}
.head-img-section {
	padding-bottom: 15px;
}
.top-header {
	display: none !important;
}
}

@media only screen and (min-width: 320px)  and (max-width: 1023px)  and (orientation: landscape) {
.slick-slide img {
	height: auto !important;
	min-height: 450px;
}
}
/* media query end */
